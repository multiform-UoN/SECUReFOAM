FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1.1e5;
        name    rampedFixedValue;   // name of generated BC

        code
        #{
            operator==(min(1.1e5, 1e5 + 1*this->db().time().value()));
        #};

        // codeInclude
        //#{
        //    #include "fvCFD.H"
        //#};

        // codeOptions
        //#{
        //    -I$(LIB_SRC)/finiteVolume/lnInclude
        //#};
    }


    outlet
    {
        type            fixedValue;
        value           uniform 1e5;
    }

    frontAndBack
    {
        type            empty;
    }
}


