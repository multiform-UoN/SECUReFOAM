FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      cC;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0;


boundaryField
{
    left
    {
        type            zeroGradient;
    }
    
    right
    {
        type            zeroGradient;
    }
    
    bottom
    {
        type             zeroGradient;
    }
    top
    {
        type             zeroGradient;

    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


